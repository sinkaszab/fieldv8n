{
  "name": "fieldv8n",
  "version": "0.0.1",
  "description": "Simple form-field/data validation.",
  "main": "dist/fieldv8n.cjs.js",
  "module": "dist/fieldv8n.esm.js",
  "browser": "dist/fieldv8n.umd.js",
  "scripts": {
    "build": "rm -rf dist; rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint --fix . && echo 'Lint complete.'",
    "prettier-check": "prettier --check src/**/*.js",
    "prettier": "prettier --write src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinkaszab/fieldv8n.git"
  },
  "keywords": [
    "validation",
    "v8n",
    "form",
    "field"
  ],
  "author": "Sinka, Szabolcs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinkaszab/fieldv8n/issues"
  },
  "homepage": "https://github.com/sinkaszab/fieldv8n#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.3"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.2"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "airbnb-base/legacy",
      "plugin:prettier/recommended",
      "plugin:jest/recommended"
    ],
    "plugins": ["jest"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/prefer-to-have-length": "warn",
      "jest/valid-expect": "error"
    }
  }
}
